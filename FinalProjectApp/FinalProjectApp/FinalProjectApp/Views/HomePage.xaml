<?xml version="1.0" encoding="utf-8" ?>
<views:BaseContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:views="clr-namespace:FinalProjectApp.Views;assembly=FinalProjectApp"
		     xmlns:resx="clr-namespace:FinalProjectApp.Resources.Resx"
             x:Class="FinalProjectApp.Views.HomePage">
    <ContentPage.Content>
        <StackLayout>
			<Button Text="{x:Static resx:AppResources.SignIn}" Command="{Binding NavigateSignInCommand}" />
			<Button Text="{x:Static resx:AppResources.Register}" Command="{Binding NavigateRegisterCommand}" />
		</StackLayout>
    </ContentPage.Content>
</views:BaseContentPage>